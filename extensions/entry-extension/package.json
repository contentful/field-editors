    "contentful-ui-extensions-sdk": "^3.12.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "emotion": "10.0.17",
    "react-sortable-hoc": "^1.11.0"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
