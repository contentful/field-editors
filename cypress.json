{
  "baseUrl": "http://localhost:9000",
  "reporter": "./node_modules/mochawesome/src/mochawesome.js",
  "reporterOptions": {
    "mochaFile": "./cypress/reports/mocha/results.xml",
    "reportDir": "./cypress/reports/separate-reports",
    "overwrite": false,
    "html": false,
    "json": true
  },
  "retries": 2,
  "numTestsKeptInMemory": 1,
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ]
}
